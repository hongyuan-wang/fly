{
  "manifest_version": 2,

  "name": "fly",
  "description": "This extension make proxying websites just you want easy",
  "version": "1.0",

  "icons": { "128": "img/logo.png" },
		  
  "browser_action": {
    "default_icon": "img/icon-auto.png",
    "default_popup": "popup.html"
  },
  "permissions": [
	"tabs",
	"proxy"
  ]
}